<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/notes_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/notes_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Notes">
        <activity android:name=".Edit_note" />
        <activity android:name=".write_note" android:theme="@style/Theme.Notes.NoActionBar"/>
        <activity android:name=".AboutMe"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>